// Maps-STL
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <set>
#include <map>
#include <algorithm>
using namespace std;


int main() {
    int Q;
    int Query;
    string Query_line;
    string name;
    int score;
    map <string, int> m;
    
    cin >> Q;
    
    for (int i = 0; i < Q; i++) {
        cin >> Query;
        
        if (Query == 1) {
            cin >> name >> score;
            map<string,int>::iterator itr = m.find(name);
            if (itr != m.end()) {
                itr->second = itr->second + score;
            }
            else {
                m.insert(make_pair(name,score));
            }
        }
        else if (Query == 2) {
            cin >> name;
            m.erase(name);
        }
        else if (Query == 3) {
            cin >> name;
            map<string,int>::iterator itr = m.find(name);
            if (itr != m.end()) {
                cout << itr->second << '\n';
            }
            else {
                cout << 0 << '\n';
            }
        }
        
    }
     
    return 0;
}

// Print Pretty
#include <iostream>
#include <iomanip> 
using namespace std;

int main() {
	int T; cin >> T;
	cout << setiosflags(ios::uppercase);
	cout << setw(0xf) << internal;
	while(T--) {
		double A; cin >> A;
		double B; cin >> B;
		double C; cin >> C;
        
        int digit;
        long long n = A;
        string hex = "";
        char polarity = '+';
        
        if (n == 0) {
            hex = "0";
        }
        else {
            while (n != 0) {
                digit = n % 16;
                n = n / 16;
                char h;
                
                if (digit < 10) {
                    h = '0' + digit;
                }
                else {
                    h = 'a' + (digit - 10);
                }
                
                hex = h + hex;
            }
        }
        
        
        hex = "0x" + hex;
        cout << setw(0) << hex << '\n';
        
        if (B > 0) {
            polarity = '+';
        }
        else if (B < 0) {
            polarity = '-';
        }
        
        int Bint = int(B);
        string BStr = polarity + to_string(Bint);
        int BStr_Length = BStr.length() + 3;
        int width = 16 - BStr_Length;
        
        cout << setfill('_') << setw(width) << polarity << fixed << setprecision(2) << B << '\n';
        
        int power = 0;
        while (C >= 10) {
            C = C / 10;
            power++;
        }
        
        string sciNot = "E+";
        
        if (power < 10) {
            sciNot = sciNot + '0' + to_string(power);
        }
        else if (power >= 10) {
            sciNot = sciNot + to_string(power);
        }
        
        cout << setprecision(9) << C << sciNot << '\n';
        
	}
	return 0;

}

