// collections.Counter()
from collections import Counter

profit = 0

X = int(input())

shoe_sizes = [int(x) for x in input().split()]
inventory = Counter(shoe_sizes)

N = int(input())

for i in range(0,N):
    shoesize_and_price = [int(x) for x in input().split()]
    size = shoesize_and_price[0] 
    price = shoesize_and_price[1]
    
    if size in Counter(shoe_sizes).keys():
        profit += price
        shoe_sizes.remove(size)
    else:
        continue
        
print(profit)

// DefaultDict Tutorial
from collections import defaultdict

a = 1

n_and_m = [int(x) for x in input().split()]
n,m = n_and_m

A_list = [input() for j in range(n)]
B_list = [input() for k in range(m)]
positions_list = []

dA = defaultdict(list)

for item in A_list:
    dA[item].append(a)
    a += 1

for item in B_list:
    if dA[item] != []:
        positions_list.append(dA[item])
    else:
        positions_list.append(-1)

for position in positions_list:
    if position != -1:
        print(*position)
    else:
        print(-1)

// Collections.namedtuple()
from collections import namedtuple

N = int(input())
fields = input().split()
i = 0

for item in fields:
    if item == "MARKS":
        index = i
        break
    else:
        i += 1
        
score = 0

for j in range(0,N):
    data = input().split()
    mark = int(data[i])
    score += mark
    
print(score / N)

// Collectections.OrderedDict()
from collections import OrderedDict

N = int(input())

od = OrderedDict()

for i in range(N):
    item_and_price = input()
    j = 0
    for char in item_and_price:
        if char.isnumeric():
            item = item_and_price[:j-1]
            price = int(item_and_price[j:])
            break
        else:
            j += 1
            
    if item in od:
        od[item].append(price)
    else:
        od[item] = [price]
        
for key in od:
    print(key + ' '+ str(sum(od[key])))

// Collections.deque()
from collections import deque

N = int(input())
d = deque()

for i in range(N):
    command_with_index = input().split()
    if len(command_with_index) == 1:
        if command_with_index[0] == 'pop':
            d.pop()
        elif command_with_index[0] =='popleft':
            d.popleft()
        
    else:
        command, index = command_with_index
    
        if command == 'append':
            d.append(index)
        elif command == 'appendleft':
            d.appendleft(index)
        
for item in d:
    print(item, end=' ')

