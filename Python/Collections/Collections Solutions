// collections.Counter()
from collections import Counter

profit = 0

X = int(input())

shoe_sizes = [int(x) for x in input().split()]
inventory = Counter(shoe_sizes)

N = int(input())

for i in range(0,N):
    shoesize_and_price = [int(x) for x in input().split()]
    size = shoesize_and_price[0] 
    price = shoesize_and_price[1]
    
    if size in Counter(shoe_sizes).keys():
        profit += price
        shoe_sizes.remove(size)
    else:
        continue
        
print(profit)

// DefaultDict Tutorial
from collections import defaultdict

a = 1

n_and_m = [int(x) for x in input().split()]
n,m = n_and_m

A_list = [input() for j in range(n)]
B_list = [input() for k in range(m)]
positions_list = []

dA = defaultdict(list)

for item in A_list:
    dA[item].append(a)
    a += 1

for item in B_list:
    if dA[item] != []:
        positions_list.append(dA[item])
    else:
        positions_list.append(-1)

for position in positions_list:
    if position != -1:
        print(*position)
    else:
        print(-1)

