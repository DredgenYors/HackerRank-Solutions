// Exceptions
T = int(input())

for i in range(T):
    values = input().split()
    a,b = values
    
    try:
        print(int(int(a) / int(b)))
    except ZeroDivisionError as e:
        print("Error Code: integer division or modulo by zero")
    except ValueError as e:
        print("Error Code:", e)

