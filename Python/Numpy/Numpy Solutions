// Eye and Identity
import numpy
numpy.set_printoptions(legacy='1.13')

NandM = list(map(int,input().split()))

N,M = NandM

print(numpy.eye(N,M))

// Array Mathematics
import numpy

NandM = list(map(int, input().split()))
A = []
B = []

N,M = NandM

for i in range(N):
    tempArray = list(map(int, input().split()))
    A.append(tempArray)
    
for i in range(N):
    tempArray = list(map(int, input().split()))
    B.append(tempArray)
    
numpyA = numpy.array(A)
numpyB = numpy.array(B)

numpyA.shape = (N,M)
numpyB.shape = (N,M)

print(numpyA + numpyB)
print(numpyA - numpyB)
print(numpyA * numpyB)
print(numpyA // numpyB)
print(numpyA % numpyB)
print(numpyA ** numpyB)

// Floor, Ceil, and Rint
import numpy
numpy.set_printoptions(legacy='1.13')

arr = list((map(float,input().split())))
numpyArr = numpy.array(arr)

print(numpy.floor(numpyArr))
print(numpy.ceil(numpyArr))
print(numpy.rint(numpyArr))

// Sum and Prod
import numpy

NandM = list(map(int,input().split()))
N,M = NandM

arr = []

for i in range(N):
    tempArr = list(map(int,input().split()))
    arr.append(tempArr)
    
numpyArr = numpy.array(arr)
numpyArr.shape = (N,M)

npyArrSum = numpy.sum(numpyArr, axis = 0)

print(numpy.prod(npyArrSum))

// Min and Max
import numpy

NandM = list(map(int,input().split()))

N,M = NandM

arr = []

for i in range(N):
    tempArr = list(map(int,input().split()))
    arr.append(tempArr)
    
numpyArr = numpy.array(arr)
numpyArr.shape = (N,M)

npyArrMin = numpy.min(numpyArr, axis = 1)
print(numpy.max(npyArrMin))

// Mean, Var, and Std
import numpy

NandM = list(map(int,input().split()))

N,M = NandM

arr = []

for i in range(N):
    tempArr = list(map(int,input().split()))
    arr.append(tempArr)
    
numpyArr = numpy.array(arr)
numpyArr.shape = (N,M)

print(numpy.mean(numpyArr, axis = 1))
print(numpy.var(numpyArr, axis = 0))

std = numpy.std(numpyArr)

print(numpy.round(std, 11))

// Dot and Cross
import numpy

N = int(input())
A = []
B = []


for i in range(N):
    tempArray = list(map(int, input().split()))
    A.append(tempArray)
    
for i in range(N):
    tempArray = list(map(int, input().split()))
    B.append(tempArray)
    
numpyA = numpy.array(A)
numpyB = numpy.array(B)

numpyA.shape = (N,N)
numpyB.shape = (N,N)

print(numpy.dot(numpyA, numpyB))

// Inner and Outer
import numpy

A = list(map(int, input().split()))
B = list(map(int, input().split()))

numpyA = numpy.array(A)
numpyB = numpy.array(B)

print(numpy.inner(A,B))
print(numpy.outer(A,B))

// Polynomials
import numpy

coeffP = list(map(float, input().split()))
x = int(input())

numpyCP = numpy.array(coeffP)

print(numpy.polyval(numpyCP,x))

