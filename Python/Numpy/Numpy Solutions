// Arrays
import numpy

def arrays(arr):
    a = numpy.array(arr, dtype = 'f')
    rev = numpy.flip(a)
    return(rev)

arr = input().strip().split(' ')
result = arrays(arr)
print(result)

// Shape and Reshape
import numpy

array = list(map(int, input().split()))

nparr = numpy.array(array)
nparr.shape = (3,3)

print(nparr)

// Transpose and Flatten
import numpy

NandM = list(map(int, input().split()))
N, M = NandM

total_arr = []

for i in range(N):
    temp_array = list(map(int,input().split()))
    
    total_arr.append(temp_array)
    
nparr = numpy.array(total_arr)

print(numpy.transpose(nparr))
print(nparr.flatten())

