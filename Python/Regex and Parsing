// Detect Floating Point Number
cases = int(input())

for i in range(cases):
    case = input()
    if case == '0':
        print('False')
        continue
    try:
        float(case)
        print('True')
    except:
        print('False')
        continue

// Re.split()
regex_pattern = r"[,.]+"	# Do not delete 'r'.

import re
print("\n".join(re.split(regex_pattern, input())))

