// No Idea!
happiness = 0

array_size_and_set_size = [int(x) for x in input().split()]
array_size = array_size_and_set_size[0]
set_size = array_size_and_set_size[1]

array = [int(x) for x in input().split()]
A = set([int(x) for x in input().split()])
B = set([int(x) for x in input().split()])

for value in array:
    if value in A:
        happiness = happiness + 1
    elif value in B:
        happiness = happiness - 1
        
print(happiness)
