// Text Wrap
import textwrap

def wrap(string, max_width):
    sub_str_list = []
    return_str = ""
    i = 0
    while i < len(string):
        sub_str_list.append(string[i:i+max_width])
        i = i + max_width
        
    for sub_str in sub_str_list:
        return_str = return_str + sub_str +'\n'
    return return_str

if __name__ == '__main__':
    string, max_width = input(), int(input())
    result = wrap(string, max_width)
    print(result)

