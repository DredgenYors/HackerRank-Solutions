// Alphabet Rangoli
import string
def print_rangoli(size):

    if size == 1:
        print('a')
        return
        
    alpha_lower = string.ascii_lowercase
    return_array = []
    return_str = ""
    width = (size * 4) - 3  # Correct width calculation
    
    for i in range(0, size):
        if i == 0:
            return_str = alpha_lower[size - 1].center(width, '-')
            return_array.insert(i, return_str)
            return_array.insert(i - 1, return_str)
        else:
            chars = alpha_lower[size - 1 - i:size]  
            mirrored = chars[::-1] + chars[1:]  
            return_str = '-'.join(mirrored).center(width, '-')
            return_array.insert(i, return_str)
            if i != size - 1:
                return_array.insert(-i - 1, return_str)
    
    for row in return_array:
        print(row)

if __name__ == '__main__':
    n = int(input())
    print_rangoli(n)

// Capitalize!
#!/bin/python3

import math
import os
import random
import re
import sys

# Complete the solve function below.
def solve(s):
    result = []
    words = s.split(" ")  
    for word in words:
        if word and word[0].isdigit():  
            result.append(word)  # Keep the word as is
        elif word: 
            result.append(word.capitalize())
        else:
            result.append("")  # Preserve empty spaces
    output_string = " ".join(result)  
    return(output_string)
    
if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)

    fptr.write(result + '\n')

    fptr.close()

// String Formatting
def print_formatted(number):
    n = number
    
    binary_final = bin(n)
    formatted_binary_final = binary_final[2:]
    len_indent = len(formatted_binary_final)
    
    for i in range(1, n + 1):
        decimal = str(i)
        
        binary = bin(i)
        formatted_binary = binary[2:]
    
        octal = oct(i)
        formatted_octal = octal[2:]
        
        hexadec = hex(i)
        formatted_hex = hexadec[2:].upper()
        
        print(f" {decimal:>{len_indent-1}} {formatted_octal:>{len_indent}} {formatted_hex:>{len_indent}} {formatted_binary:>{len_indent}}")
        
    
if __name__ == '__main__':
    n = int(input())
    print_formatted(n)

